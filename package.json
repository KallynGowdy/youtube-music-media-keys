{
    "name": "youtube-music-media-keys",
    "version": "1.0.0",
    "description": "A chrome extension that adds media keys support to YouTube Music",
    "private": true,
    "main": "index.js",
    "scripts": {
        "build": "tsc",
        "watch": "tsc -w",
        "pack": "npm run build && crx pack ./extension -o youtube-music-media-keys.crx"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/KallynGowdy/youtube-music-media-keys.git"
    },
    "author": "Kallyn Gowdy <kallyngowdy@gmail.com>",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/KallynGowdy/youtube-music-media-keys/issues"
    },
    "homepage": "https://github.com/KallynGowdy/youtube-music-media-keys#readme",
    "devDependencies": {
        "husky": "^4.3.0",
        "prettier": "^2.1.2",
        "pretty-quick": "^3.1.0",
        "typescript": "4.0.3",
        "crx": "5.0.1"
    },
    "husky": {
        "hooks": {
            "pre-commit": "pretty-quick --staged"
        }
    }
}
